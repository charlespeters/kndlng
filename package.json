{
  "name": "kndlng",
  "version": "0.0.1",
  "description": "Obsidian.css represented in JavaScript",
  "main": "dist/kindling.js",
  "scripts": {
    "bundle:dev": "webpack example.js ./dist/bundle.js",
    "bundle:prod": "webpack index.js ./dist/kindling.js -p",
    "test": "echo \"Error: no test specified\" && exit 1",
    "server": "webpack-dev-server --hot --open --colors --content-base dist/",
    "watch": "npm run bundle:dev -- -w",
    "start": "npm-run-all --parallel watch server",
    "prestart": "npm run bundle:dev",
    "prepublish": "npm run bundle:prod",
    "lint": "standard 'index.js' 'test.js' 'lib/**/*.js' --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/charlespeters/kndlng.git"
  },
  "author": "charles peters <charlespeters42@gmail.com> (https://charlespeters.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/charlespeters/kndlng/issues"
  },
  "homepage": "https://github.com/charlespeters/kndlng#readme",
  "dependencies": {
    "aphrodite": "^1.1.0",
    "obsidian.css": "^2.1.3"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.5",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-register": "^6.18.0",
    "json-loader": "^0.5.4",
    "npm-run-all": "^3.1.0",
    "onchange": "^3.0.0",
    "snazzy": "^5.0.0",
    "source-map-loader": "^0.1.5",
    "standard": "^8.0.0",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "standard": {
    "globals": [
      "HTMLElement"
    ]
  },
  "keywords": [
    "js",
    "styles",
    "css"
  ]
}
